<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.aplikasikontak">

    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.WRITE_SMS" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Kontak"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AplikasiKontak">
        <activity
            android:name="com.example.utskontak.AboutActivity"
            android:exported="true" />
        <activity
            android:name="com.example.utskontak.UpdateKontak"
            android:exported="true" />
        <activity
            android:name="com.example.utskontak.LihatKontak"
            android:exported="true" />
        <activity
            android:name="com.example.utskontak.BuatKontak"
            android:exported="true" />
        <activity
            android:name="com.example.utskontak.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>